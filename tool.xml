<tool force_history_refresh="false" hidden="false" name="aragorn" version="1.2.36" workflow_compatible="true">
  <description>tRNA finder</description>
  <stdio>
    <exit_code level="fatal" range="1:"/>
  </stdio>
  <command><![CDATA[aragorn $flag
-float $float
-i$int_min,$int_max

-output $output]]></command>
  <inputs>
    <param checked="false" help="(-flag) Flag help" label="Flag label" type="boolean" truevalue="$flag"/>
    <param help="(-float) Float help" label="Float label" type="float" value="0"/>
    <param help="(-int_min) int_min help" label="int_min label" type="integer" value="0"/>
    <param help="(-int_max) int_max help" label="int_max label" type="integer" value="0"/>
  </inputs>
  <outputs>
    <data format="tabular" hidden="false" name="output"/>
  </outputs>
  <help><![CDATA[HI]]></help>
</tool>
